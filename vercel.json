{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "static/**", "use": "@vercel/static" },
    { "src": "config.js", "use": "@vercel/static" }
  ],
  "rewrites": [
    { "source": "/**", "destination": "/server.js" }

  ]
}
