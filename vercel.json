{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" },
    { "src": "**/*.html", "use": "@vercel/static" },
    { "src": "static/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/mobile/login" },

    { "src": "^/mobile/login$", "dest": "/server.js" },
    { "src": "^/mobile/api/(.*)$", "dest": "/server.js" },

    { "src": "^/mobile$", "dest": "/login.html" },
    { "src": "^/mobile/home$", "dest": "/home.html" },

    { "src": "^/mobile/profile$", "dest": "/profile.html" },
    { "src": "^/mobile/calendar$", "dest": "/calendar.html" },
    { "src": "^/mobile/checkin$", "dest": "/checkin.html" },

    { "src": "^/mobile/workflow$", "dest": "/workflow.html" },

    { "src": "^/mobile/leave/transactions$", "dest": "/leave_transactions.html" },
    { "src": "^/mobile/leave/details$", "dest": "/leave_details.html" },

    { "src": "^/mobile/document/transactions$", "dest": "/document_transactions.html" },
    { "src": "^/mobile/document/details$", "dest": "/document_details.html" },

    { "src": "^/mobile/excuse/transactions$", "dest": "/excuse_transactions.html" },
    { "src": "^/mobile/excuse/details$", "dest": "/excuse_details.html" },

    { "src": "^/mobile/reimbursement/transactions$", "dest": "/reimbursement_transactions.html" },
    { "src": "^/mobile/reimbursement/details$", "dest": "/reimbursement_details.html" },

    { "src": "^/mobile/flight/transactions$", "dest": "/flight_transactions.html" },
    { "src": "^/mobile/flight/details$", "dest": "/flight_details.html" },

    { "src": "^/mobile/business/transactions$", "dest": "/business_transactions.html" },
    { "src": "^/mobile/business/details$", "dest": "/business_details.html" },

    { "src": "^/mobile/onbehalf/leave$", "dest": "/onbehalf_leave.html" },
    { "src": "^/mobile/onbehalf/excuse$", "dest": "/onbehalf_excuse.html" },
    { "src": "^/mobile/onbehalf/document$", "dest": "/onbehalf_document.html" },
    { "src": "^/mobile/onbehalf/reimbursement$", "dest": "/onbehalf_reimbursement.html" },
    { "src": "^/mobile/onbehalf/flight$", "dest": "/onbehalf_flight.html" },
    { "src": "^/mobile/onbehalf/business$", "dest": "/onbehalf_business.html" },

    { "src": "^/mobile/approvals/leave/pending$", "dest": "/pending_leave_transactions.html" },
    { "src": "^/mobile/approvals/excuse/pending$", "dest": "/pending_excuse_transactions.html" },
    { "src": "^/mobile/approvals/document/pending$", "dest": "/pending_document_transactions.html" },
    { "src": "^/mobile/approvals/reimbursement/pending$", "dest": "/pending_reimbursement_transactions.html" },
    { "src": "^/mobile/approvals/flight/pending$", "dest": "/pending_flight_transactions.html" },
    { "src": "^/mobile/approvals/business/pending$", "dest": "/pending_business_transactions.html" },

    { "src": "^/mobile/team/hierarchy$", "dest": "/team_hierarchy.html" },
    { "src": "^/mobile/team/calendar$", "dest": "/team_calendar.html" }
  ]
}
